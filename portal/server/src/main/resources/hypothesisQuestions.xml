    </ns0:hasQuestionVariable>

    <ns0:hasQuestionVariable>
      <ns0:QuestionVariable rdf:about="http://disk-project.org/resources/question/HQ3V4">
        <ns0:hasVariableName>?Demographic</ns0:hasVariableName>
        <ns0:hasConstraints>?Demographic a neuro:DemographicCharacteristic </ns0:hasConstraints>
      </ns0:QuestionVariable>
    </ns0:hasQuestionVariable>

  </ns0:Question>

</rdf:RDF>
